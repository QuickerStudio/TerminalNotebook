{
  "compilerOptions": {
	"module": "Node16",
	"target": "ES2022",
	"lib": [
	  "ES2022",
	  "DOM"
	],
	"sourceMap": true,
	"rootDir": "src",
	"strict": true,
	"jsx": "react-jsx",
	"esModuleInterop": true,
	"resolveJsonModule": true,
	"skipLibCheck": true,
	"forceConsistentCasingInFileNames": true,
	"outDir": "out"
  },
  "include": [
	"src"
  ]
}
